<body>
  <h1>NOK <%=id%></h1>
  <h2></h2>
  <div class="container">

    <div class="row">
      <div class="col s12 m10">
        <div class="row">
          <form class="" action="http://localhost:8000/nok/<%=idArray[0]._id%>?_method=put" method="post">
            <!-- <form class="" action="http://localhost:8000/update/<%=idArray[0]._id%>?_method=put" method="post"> -->

            <div class="input-field col s3">
            <select id="rank" name="rank">
              <option value="<%=idArray[0].rank%>" selected><%=idArray[0].rank%></option>
              <option value="Wife">Wife</option>
              <option value="Husband">Husband</option>
              <option value="Son">Son</option>
              <option value="Dauther">Dauther</option>
              <option value="Mother">Mother</option>
              <option value="Father">Father</option>
              <option value="GrandMother">Grand Mother</option>
              <option value="GrandFather">GrandFather</option>
              <option value="GrandDauther">GrandDauther</option>
              <option value="GrandSon">GrandSon</option>
            </select>
              <label>Rank</label>
            </div>
            <div class="input-field col s3">
              <input id="first_name" type="text" name="name" class="validate" value="<%=idArray[0].name%>">
              <label for="first_name">Name</label>
            </div>
            <div class="input-field col s3">
              <input id="last_name" type="text" name="family" class="validate" value="<%=idArray[0].family%>">
              <label for="last_name">Family</label>
            </div>
            <div class="input-field col s3">
              <input id="middle_name" type="text" name="middle" class="validate" value="<%=idArray[0].middle%>">
              <label for="middle_name">Middle</label>
            </div>
        <!-- </div> -->

          <div class="input-field col s3">
            <input id="birthdate" name="birthdate" type="text" class="validate" value="<%=idArray[0].birthdate%>">
            <label for="birthdate">Birth DD mon YYYY</label>
          </div>
          <div class="input-field col s3">
            <input id="age" name="age" type="number" class="validate" value="<%=idArray[0].age%>">
            <label for="age">Age</label>
          </div>

          <div class="input-field col s3">
            <input id="telephone" type="text" name="tel" class="validate" value="<%=idArray[0].tel%>">
            <label for="telephone">Telephone</label>
          </div>
          <div class="input-field col s3">
            <input id="email" type="text" name="email" class="validate" value="<%=idArray[0].email%>">
            <label for="email">e-mail</label>
          </div>
          <div class="input-field col s3">
            <select id="nationality" name="nationality">
              <option value="<%=idArray[0].nationality%>" disabled selected><%=idArray[0].nationality%></option>
              <option value="UKR">UKR</option>
              <option value="RUS">RUS</option>
              <option value="NDL">NDL</option>
              <option value="UK">UK</option>
              <option value="USA">USA</option>
            </select>
            <label>Nationality</label>
          </div>
          <div class="input-field col s3">
            <select id="citizenship" name="citizenship">
              <option value="<%=idArray[0].citizenship%>" disabled selected><%=idArray[0].citizenship%></option>
              <option value="Citizen">Citizen</option>
              <option value="Allien">Allien</option>
              <option value="Non citizen">Non citizen</option>
              <option value="Stateless">Stateless</option>
            </select>
            <label>Citizenship</label>
          </div>
          <div class="input-field col s3">
            <select id="maritalstatus" name="maritalstatus">
              <option value="<%=idArray[0].maritalstatus%>" disabled selected><%=idArray[0].maritalstatus%></option>
              <option value="Single">Single</option>
              <option value="Married">Married</option>
              <option value="Divorced">Divorced</option>
              <option value="Live together">Live together</option>
              <option value="Widoved">Widoved</option>
              <option value="Separated">Separated</option>
            </select>
            <label>Marital status</label>
          </div>
          <div class="input-field col s3">
            <input id="taxation_number" type="text" name="taxation" class="validate" value="<%=idArray[0].taxation%>">
            <label for="taxation_number">Taxation number</label>
          </div>
        </div>
        <hr>
        <input class="col m2 btn-small red" type="submit" name="section_basic" value="Save">
        </form>
        <div class="row">
          <form class="col m2" action="http://localhost:8000/nok/<%=idArray[0]._id%>" method="post">
            <input class="btn-small pink" type="submit" name="nok" value="Next of kin">
          </form>
          <form class="col m2" action="http://localhost:8000/cert/<%=idArray[0]._id%>" method="post">
            <input class="btn-small pink" type="submit" name="cert" value="Certificate">
          </form>
          <form class="col m2" action="http://localhost:8000/exp/<%=idArray[0]._id%>" method="post">
            <input class="btn-small pink" type="submit" name="exp" value="Seagoing">
          </form>
          <a href="/main" class="col m2 waves-effect waves-light btn-small blue">Main page</a>
        </div>

        <script type="text/javascript">
          var bd = document.getElementById("birthdate");
          bd.onkeydown = function() {
            var convertedBD = Date.parse(bd.value)
            var nowDate = Date.now();
            var age = document.getElementById("age");
            age.value = 0;
            var total = ((nowDate - convertedBD) / 31718000000);
            age.value = total.toFixed(0)
            // console.log(parseInt(bdArr[2].regexp()));
          };
        </script>

      </div>
      <div class="col s12 m2">
        <div class="card">
          <div class="card-image">
            <img src="/images/side400x400.png">
            <span class="card-title">Feature &#8734;</span>
            <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">&#43;</i></a>
          </div>
          <div class="card-content">
            <p>13.04.2018</p>
          </div>
        </div>
        <!-- start option -->
        <form action="#">
          <p>
            <label>
              <input name="group1" class="with-gap" type="radio" checked />
              <span>Male</span>
            </label>
          </p>
          <p>
            <label>
              <input name="group1" class="with-gap" type="radio" />
              <span>Female</span>
            </label>
          </p>
        </form>
        <!-- end option -->
      </div>
    </div>

  </div>

</body>
<script type="text/javascript">
  // var elem = document.querySelector('select');
  // var instance = M.FormSelect.init(elem, options);

  // Or with jQuery

  $(document).ready(function() {
    $('#rank').formSelect();
    $('#stars').formSelect();
    $('#expirestatus').formSelect();
    $('#nationality').formSelect();
    $('#citizenship').formSelect();
    $('#maritalstatus').formSelect();
    $('.datepicker').datepicker();
  });
</script>

</html>
