  // Or with jQuery

  $(document).ready(function() {
    $('#rank').formSelect();
    $('#stars').formSelect();
    $('#expirestatus').formSelect();
    $('#nationality').formSelect();
    $('#citizenship').formSelect();
    $('#maritalstatus').formSelect();
    $('.datepicker').datepicker();
  });
</script>

</html>
