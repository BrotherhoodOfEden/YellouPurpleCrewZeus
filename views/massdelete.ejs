  <body>
    <style media="screen">
      .rounded-corners{
        font-weight: 300;
        font-size: 0.8rem;
        height: 14px;
        border: 1px solid #A14BFE;;
        border-radius: 2px;
        margin:4px;
        background-color: #A14BFE;
        color: #fff;
      }
      .progress{
        background-color: #FFFF92;
      }
      .determinate{
          background-color: #A14BFE!important;
      }
      .collection a.collection-item{
        color: #000!important;
      }
      .dorange{
        background-color:#814D01;
      }
      .dgreen{
        background-color:#018132;
      }
    </style>
    <% for (var i = 0; i < idArray.length; i++) { %>
      <div class="collection">
        <a href="http://localhost:8000/read/<%=idArray[i]._id%>" class="collection-item" id="drag1" width="80" height="20"><span class="new badge blue" data-badge-caption="<%=idArray[i].basic.rank%>">
        </span><%=idArray[i].basic.name%>&nbsp<%=idArray[i].basic.family%><span class="rounded-corners"><%=idArray[i].basic.age%></span><span class="rounded-corners"><%=idArray[i].basic.stars%></span><span class="rounded-corners black">9980&#8364;</span><span class="rounded-corners dgreen">&#566; <%=idArray[i].basic.tel%></span><span class="rounded-corners dorange">@<%=idArray[i].basic.email%></span></span>
          <div class="progress">
            <div class="determinate" style="width: 10%"></div>
          </div>
        </a>
        <form class="" action="http://localhost:8000/massdelete/<%=idArray[i]._id%>" method="post">
          <input class="btn-small red" type="submit" value="Delete">
        </form>
      </div>
      <%}%>
  </body>
