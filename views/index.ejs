                  <input type="range" id="test5" min="20" max="70" /><span>Age from:</span> <span id="indicator0">20</span>
                  <script type="text/javascript">
                  let x = document.getElementById("test5");
                  let xx = document.getElementById("indicator0");
                  x.onclick = function (){
                    xx.innerHTML=x.value;
                    document.getElementById("fromA").value = x.value ;
                    console.log(xx+"Slider value");
                  };
                </script>
              </p>
            </label>
            <label for="test6">
              <p class="range-field">
                <input type="range" id="test6" min="20" max="70" /><span>Age to:</span> <span id="indicator1">20</span>
                <script type="text/javascript">
                let xa = document.getElementById("test6");
                let xxa = document.getElementById("indicator1");

                xa.onclick = function (){
                  xxa.innerHTML=xa.value;
                  document.getElementById("toB").value = xa.value ;
                  console.log(xxa+"Slider value");
                };
              </script>
              </label>

            </p>
