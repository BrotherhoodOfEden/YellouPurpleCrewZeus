<body>
  <h1>Crew <%=id%></h1>
  <h2><%=saved%></h2>
  <div class="container">

    <div class="row">
      <div class="col s12 m10">
        <div class="row">
          <form class="" action="http://localhost:8000/update/<%=idArray[0]._id%>?_method=put" method="post">
            <!-- <form class="" action="http://localhost:8000/update/<%=idArray[0]._id%>?_method=put" method="post"> -->

            <div class="input-field col s3">
            <select id="rank" name="rank">
              <option value="<%=idArray[0].basic.rank%>" selected><%=idArray[0].basic.rank%></option>
              <option value="Master">Master</option>
              <option value="ChOff">Ch.Off</option>
              <option value="2ndOff">2nd.Off</option>
              <option value="3rdOff">3rd.Off</option>
              <option value="ChEng">Ch.Eng</option>
              <option value="2ndEng">2nd.Eng</option>
              <option value="3rdEng">3rd.Eng</option>
              <option value="MMOil">MM/Oil</option>
              <option value="Bosun">Bosun</option>
              <option value="AB">AB</option>
              <option value="OS">OS</option>
            </select>
              <label>Rank</label>
            </div>
            <div class="input-field col s3">
              <input id="first_name" type="text" name="name" class="validate" value="<%=idArray[0].basic.name%>">
              <label for="first_name">Name</label>
            </div>
            <div class="input-field col s3">
              <input id="last_name" type="text" name="family" class="validate" value="<%=idArray[0].basic.family%>">
              <label for="last_name">Family</label>
            </div>
            <div class="input-field col s3">
              <input id="middle_name" type="text" name="middle" class="validate" value="<%=idArray[0].basic.middle%>">
              <label for="middle_name">Middle</label>
            </div>
        <!-- </div> -->

          <div class="input-field col s3">
            <input id="availabledate" name="availabledate" type="text" class="datepicker" value="<%=idArray[0].basic.availabledate%>">
            <label for="availabledate"> Date available</label>
          </div>

          <div class="input-field col s3">
            <input id="birthdate" name="birthdate" type="text" class="validate" value="<%=idArray[0].basic.birthdate%>">
            <label for="birthdate">Birth DD mon YYYY</label>
          </div>
          <div class="input-field col s3">
            <input id="age" name="age" type="number" class="validate" value="<%=idArray[0].basic.age%>">
            <label for="age">Age</label>
          </div>

          <div class="input-field col s3">
            <select id="stars" name="stars">
              <option value="<%=idArray[0].basic.stars%>" disabled selected><%=idArray[0].basic.stars%></option>
              <option value="&#9679; &#9675; &#9675; &#9675; &#9675;">&#9679; &#9675; &#9675; &#9675; &#9675;</option>
              <option value="&#9679; &#9679; &#9675; &#9675; &#9675;">&#9679; &#9679; &#9675; &#9675; &#9675;</option>
              <option value="&#9679; &#9679; &#9679; &#9675; &#9675;">&#9679; &#9679; &#9679; &#9675; &#9675;</option>
              <option value="&#9679; &#9679; &#9679; &#9679; &#9675;">&#9679; &#9679; &#9679; &#9679; &#9675;</option>
              <option value="&#9679; &#9679; &#9679; &#9679; &#9679;">&#9679; &#9679; &#9679; &#9679; &#9679;</option>
            </select>
            <label>Stars</label>
          </div>


          <div class="input-field col s3">
            <input id="salary" type="text" name="salary" class="validate" value="<%=idArray[0].basic.salary%>">
            <label for="salary">Salary</label>
          </div>
          <div class="input-field col s3">
            <input id="telephone" type="text" name="tel" class="validate" value="<%=idArray[0].basic.tel%>">
            <label for="telephone">Telephone</label>
          </div>
          <div class="input-field col s3">
            <input id="email" type="text" name="email" class="validate" value="<%=idArray[0].basic.email%>">
            <label for="email">e-mail</label>
          </div>
          <div class="input-field col s3">
            <input id="expire" type="text" name="expirestatus" class="validate" value="<%=idArray[0].basic.expirestatus%>">
            <label for="expire">Exp first doc</label>
          </div>

          <div class="input-field col s3">
            <select id="nationality" name="nationality">
              <option value="<%=idArray[0].basic.nationality%>" disabled selected><%=idArray[0].basic.nationality%></option>
              <option value="UKR">UKR</option>
              <option value="RUS">RUS</option>
              <option value="NDL">NDL</option>
              <option value="UK">UK</option>
              <option value="USA">USA</option>
            </select>
            <label>Nationality</label>
          </div>
          <div class="input-field col s3">
            <select id="citizenship" name="citizenship">
              <option value="<%=idArray[0].basic.citizenship%>" disabled selected><%=idArray[0].basic.citizenship%></option>
              <option value="Citizen">Citizen</option>
              <option value="Allien">Allien</option>
              <option value="Non citizen">Non citizen</option>
              <option value="Stateless">Stateless</option>
            </select>
            <label>Citizenship</label>
          </div>
          <div class="input-field col s3">
            <select id="maritalstatus" name="maritalstatus">
              <option value="<%=idArray[0].basic.maritalstatus%>" disabled selected><%=idArray[0].basic.maritalstatus%></option>
              <option value="Single">Single</option>
              <option value="Married">Married</option>
              <option value="Divorced">Divorced</option>
              <option value="Live together">Live together</option>
              <option value="Widoved">Widoved</option>
              <option value="Separated">Separated</option>
            </select>
            <label>Marital status</label>
          </div>
          <div class="input-field col s3">
            <input id="taxation_number" type="text" name="taxation" class="validate" value="<%=idArray[0].basic.taxation%>">
            <label for="taxation_number">Taxation number</label>
          </div>
        </div>
        <hr>
        <input class="col m2 btn-small red" type="submit" name="section_basic" value="Save">
        </form>
        <div class="row">
          <form class="col m2" action="http://localhost:8000/nok/<%=idArray[0]._id%>" method="post">
            <input class="btn-small pink" type="submit" name="nok" value="Next of kin">
          </form>
          <form class="col m2" action="http://localhost:8000/cert/<%=idArray[0]._id%>" method="post">
            <input class="btn-small pink" type="submit" name="cert" value="Certificate">
          </form>
          <form class="col m2" action="http://localhost:8000/exp/<%=idArray[0]._id%>" method="post">
            <input class="btn-small pink" type="submit" name="exp" value="Seagoing">
          </form>
          <a href="/main" class="col m2 waves-effect waves-light btn-small blue">Main page</a>
        </div>




        <script type="text/javascript">
          var bd = document.getElementById("birthdate");
          bd.onkeydown = function() {
            var convertedBD = Date.parse(bd.value)
            var nowDate = Date.now();
            var age = document.getElementById("age");
            age.value = 0;
            var total = ((nowDate - convertedBD) / 31718000000);
            age.value = total.toFixed(0)
            // console.log(parseInt(bdArr[2].regexp()));
          };
        </script>

      </div>
      <div class="col s12 m2">
        <div class="card">
          <div class="card-image">
            <img src="/images/side400x400.png">
            <span class="card-title">Feature &#8734;</span>
            <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">&#43;</i></a>
          </div>
          <div class="card-content">
            <p>13.04.2018</p>
          </div>
        </div>
        <!-- start option -->
        <form action="#">
          <p>
            <label>
              <input name="group1" class="with-gap" type="radio" checked />
              <span>Male</span>
            </label>
          </p>
          <p>
            <label>
              <input name="group1" class="with-gap" type="radio" />
              <span>Female</span>
            </label>
          </p>
        </form>
        <!-- end option -->
      </div>
    </div>

  </div>

</body>
<script type="text/javascript">
  // var elem = document.querySelector('select');
